program ::= expression;

expression ::= literal
            | unary
            | binary
            | grouping
            | property
            | IDENTIFIER;

regexp ::= `\w`;
property ::= expression "." IDENTIFIER;
literal  ::= NUMBER | STRING | IDENTIFIER | "true" | "false" | "nil"
grouping ::= "(" expression ")" ;
unary    ::= ("-" | "!") expression ;
binary   ::= expression operator expression
operator ::= "==" | "!="
          | "<=" | "<" | ">=" | ">"
          | "*" | "/" | "+" | "-";